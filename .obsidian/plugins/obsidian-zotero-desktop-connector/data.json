{
  "database": "Zotero",
  "noteImportFolder": "2 Research",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite",
      "format": "template",
      "template": "{{citekey}}"
    }
  ],
  "exportFormats": [
    {
      "name": "Import Paper",
      "outputPathTemplate": "2 Research/{{creators[0].lastName}} {{date | format(\"YYYY\")}} - {{title}}.md",
      "imageOutputPathTemplate": "Files/{{citekey}}/",
      "imageBaseNameTemplate": "{{citekey}}",
      "templatePath": "Templates/Research article.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "first-imported-note",
  "shouldConcat": true
}